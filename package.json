{
  "name": "direxecute",
  "displayName": "Direxecute",
  "description": "Direct execution between Visual Studio Code and Roblox using WebSockets",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./compiled/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "direxecute.close",
        "title": "Direxecute ~ Close WebSocket connection"
      },
      {
        "command": "direxecute.manualChoose",
        "title": "Direxecute ~ Choose a file"
      },
      {
        "command": "direxecute.begin",
        "title": "Direxecute ~ Start WebSocket connection"
      },
      {
        "command": "direxecute.autoChoose",
        "title": "Direxecute ~ Run focused file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "ws": "^8.18.3"
  }
}
